  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/ssh2": "^0.5.50",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.10"
  }
