{
  "name": "node-ssh-forward",
  "version": "0.7.2",
  "main": "dist/Connection.js",
  "author": "Martin Wentzel <wentzel@stocard.de>",
  "license": "MIT",
  "description": "Easy to use ssh client with port forwarding and bastion host support",
  "dependencies": {
    "debug": "^4.3.3",
    "ssh2": "^1.5.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
